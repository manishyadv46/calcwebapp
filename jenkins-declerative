pipeline{
    agent any
    
    environment{
    path=" /opt/maven/apache-maven-3.6.3/bin"
    }
        stages{
            stage("git checkout"){
                
            
            steps{
                git 'https://github.com/manishyadv46/calcwebapp.git'
            }    
            }
            stage("maven build"){
                steps{
                sh "mvn clean package"
            }
            
            
            }
        }
    
}
